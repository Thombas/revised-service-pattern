{
    "name": "thombas/revised-service-pattern",
    "description": "A package that adds a service pattern which extends from the established models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Fielding",
            "email": "thombas.fielding@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.12",
        "laravel/framework": ">=10.0"
    },
    "autoload": {
        "psr-4": {
            "Thombas\\RevisedServicePattern\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thombas\\RevisedServicePattern\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thombas\\RevisedServicePattern\\PackageServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.7",
        "orchestra/testbench": "^10.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}